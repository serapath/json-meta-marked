{
  "name": "json-meta-marked",
  "version": "1.3.0",
  "description": "Convert markdown with json front matter to json object with metadata & html",
  "main": "index.js",
  "bin": {
    "jmm": "index.js"
  },
  "scripts": {
    "test": "opn test/index.html & watchify test/test.js -o test/bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serapath/json-meta-marked.git"
  },
  "keywords": [
    "json",
    "frontmatter",
    "front-matter",
    "front",
    "matter",
    "markdown",
    "marked",
    "meta",
    "json-front-matter"
  ],
  "author": "serapath <dev@serapath.de> (http://www.github.com/serapath)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serapath/json-meta-marked/issues"
  },
  "homepage": "https://github.com/serapath/json-meta-marked#readme",
  "dependencies": {
    "exemethod": "^1.1.4",
    "html2markdown": "^1.1.0",
    "json-matter": "^1.0.2",
    "marked": "^0.3.3",
    "babelify": "^7.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.5.0",
    "opn-cli": "^3.1.0",
    "watchify": "^3.7.0"
  }
}
